emptyrule:: all

NEUTCOREVER	= 5.1.1
NUCEFFVER   = 5.1.0
NUCCORVER   = 1.1.0
PARTNUCKVER = 5.0.5
SKMCSVCVER  = 5.0.5

COREDIR =	../neutcore/Linux_pc
LIBDIRS =	-L${COREDIR}            -L../nuccorspl/Linux_pc \
			-L../nuceff/Linux_pc -L../partnuck/Linux_pc  \
			-L../skmcsvc/Linux_pc

MCLIB = ${LIBDIRS} -lneutcore_${NEUTCOREVER} -lneutcore_${NEUTCOREVER} \
		-lskmcsvc_${SKMCSVCVER}	\
		-lnuceff_${NUCEFFVER} -lnuccorrspl_${NUCCORVER} \
		-lpartnuck_${PARTNUCKVER} -lskmcsvc_${SKMCSVCVER}

OBJS	= tpispir.o

INCLUDES	= -I

#FDEBUGFLAGS = 

FortranProgramTarget(calcapicrs,calcapicrs.o $(OBJS),,$(MCLIB) `cernlib geant321 graflib grafX11 packlib kernlib mathlib pdflib804`,-lX11)

InstallProgram(calcapicrs,$(BINDIR))
