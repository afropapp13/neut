LIBALIAS = nuceff
VER  = 5.3.2.1
IVER = 5321

LIBNAME = $(LIBALIAS)_$(VER)

FORTRANDEFINES += -DNENUCEVER=$(IVER)

XCOMM ------------------------------------------------------
XCOMM Define Object Files
XCOMM ------------------------------------------------------

FOBJ = efabpath.o efabrho.o efabsrat.o efcalamp.o efcohsct.o \
